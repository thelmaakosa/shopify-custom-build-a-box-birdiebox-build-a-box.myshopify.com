{
  "sections": {
    "collection-header": {
      "type": "collection-header",
      "custom_css": [
        "a:link {color: #18c9c9; background-color: transparent; text-decoration: underline;}",
        "a:visited {color: #18c9c9; background-color: transparent; text-decoration: none;}",
        "a:hover {color: #018a8f; background-color: transparent; text-decoration: underline;}"
      ],
      "settings": {
        "enable": true,
        "collection_image_enable": false,
        "parallax": false,
        "parallax_direction": "top"
      }
    },
    "main-collection": {
      "type": "main-collection",
      "blocks": {
        "collection_description": {
          "type": "collection_description",
          "settings": {}
        },
        "product_grid": {
          "type": "product_grid",
          "settings": {
            "grid_style": "small"
          }
        }
      },
      "block_order": [
        "collection_description",
        "product_grid"
      ],
      "custom_css": [
        "a:link {color: #000000; background-color: transparent; text-decoration: underline;}",
        "a:visited {color: #000000; background-color: transparent; text-decoration: none;}",
        "a:hover {color: #18c9c9; background-color: transparent; text-decoration: underline;}"
      ],
      "settings": {
        "enable_sidebar": true,
        "collapse_filters": true,
        "enable_color_swatches": false,
        "enable_sort": true
      }
    },
    "recently-viewed": {
      "type": "recently-viewed",
      "settings": {}
    }
  },
  "order": [
    "collection-header",
    "main-collection",
    "recently-viewed"
  ]
}